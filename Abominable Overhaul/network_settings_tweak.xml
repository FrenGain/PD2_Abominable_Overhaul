<?xml version="1.0"?>
<tweaks>
	<tweak version="2" name="settings/network" extension="network_settings">
		<search>
			<network/>
			<rpc/>
			<message name="server_drop_carry"/>
			<param type="int" min="0" max="1"/>
		</search>
		<target mode="attributes">
			<attr name="max" value="2"/>
		</target>
	</tweak>

	<tweak version="2" name="settings/network" extension="network_settings">
		<search>
			<network/>
			<rpc/>
			<message name="sync_carry_data"/>
			<param type="int" min="0" max="1"/>
		</search>
		<target mode="attributes">
			<attr name="max" value="2"/>
		</target>
	</tweak>
</tweaks>